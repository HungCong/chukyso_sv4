@model IEnumerable<WebDT.Models.menu>
<ul class="nav navbar-nav">
    <li>@Html.ActionLink("Trang chủ", "Index", "Default")</li>


    @foreach (var i in Model)
    {
        <li>
                <a href="@ViewBag.meta/@i.meta/" style="font-weight: bold">@i.name</a>
                @if (i.name == "Cửa Hàng")
                {
                     <ul>
                         @{Html.RenderAction("getCategory1", "Category");      }
                     </ul>
                }
        </li>
    }
</ul>

